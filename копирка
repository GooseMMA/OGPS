n = int(input())

left = n * n
right = (n + 1) * (n + 1)

# Решето строим до right
N = right
primes = [i for i in range(N + 1)]
primes[1] = 0
i = 2
while i <= N:
    if primes[i] != 0:
        j = i + i
        while j <= N:
            primes[j] = 0
            j += i
    i += 1

count = 0
num = left
while num <= right:
    if primes[num] != 0:
        count += 1
    num += 1

print(count)